GOID<-tapply(tmp[,1],as.factor(tmp[,2]),function(x) x)
GeneID<-tapply(tmp[,2],as.factor(tmp[,1]),function(x) x)
write.gmt <- function(geneSet=GeneID,gmt_file='GO.gmt'){
sink( gmt_file )
for (i in 1:length(geneSet)){
cat(names(geneSet)[i])
cat('\tNA\t')
cat(paste(geneSet[[i]],collapse = '\t'))
cat('\n')
}
sink()
}
